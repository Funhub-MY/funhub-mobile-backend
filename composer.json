{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "algolia/algoliasearch-client-php": "^3.3",
        "aws/aws-sdk-php": "^3.263",
        "bezhansalleh/filament-shield": "^2.4",
        "bmatovu/laravel-xml": "^3.2",
        "doctrine/dbal": "^3.6",
        "filament/filament": "^2.0",
        "filament/spatie-laravel-media-library-plugin": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "kreait/laravel-firebase": "^5.2",
        "laravel-notification-channels/fcm": "^3.2",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.1",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.12",
        "phpjuice/opencf": "^2.2",
        "predis/predis": "^2.1",
        "psr/simple-cache": "^2.0",
        "pxlrbt/filament-excel": "^1.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "knuckleswtf/scribe": "dev-master",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "laravel/telescope": "^4.13",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "pestphp/pest": "^1.23",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/CloudflareHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "8.1.15"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
